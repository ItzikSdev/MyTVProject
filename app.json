{
  "expo": {
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/tv_icons/icon-400x240.jpg",
          "appleTVImages": {
            "icon": "./assets/tv_icons/icon-1280x768.jpg",
            "iconSmall": "./assets/tv_icons/icon-400x240.jpg",
            "iconSmall2x": "./assets/tv_icons/icon-800x480.jpg",
            "topShelf": "./assets/tv_icons/icon-1920x720.jpg",
            "topShelf2x": "./assets/tv_icons/icon-3840x1440.jpg",
            "topShelfWide": "./assets/tv_icons/icon-2320x720.jpg",
            "topShelfWide2x": "./assets/tv_icons/icon-4640x1440.jpg"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ],
      "expo-router",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "name": "rtv-project",
    "slug": "rtv-project",
    "android": {
      "package": "com.itziksavaia.MyTVProject"
    }
  }
}
